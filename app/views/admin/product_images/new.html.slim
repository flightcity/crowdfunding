-title = '商品画像の新規登録'
h1 = title

= form_for [ :admin, @product, @product_image ] do |f|
  == render 'form', f: f
  .buttons
    = button_tag type: 'submit', class: 'btn btn-primary' do
      i.fa.fa-plus-circle>
      | 登録
    = link_to 'キャンセル', :admin_product_product_images, class: 'btn btn-default'