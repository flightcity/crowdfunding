<% @title = '商品画像の編集' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for [ :admin, @product, @product_image ] do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons">
      <%= f.submit '更新' %>
      <%= link_to 'キャンセル', admin_product_product_images_path(:product_id => @product_image.product_id) %>
    </div>
  <% end %>
</div>
