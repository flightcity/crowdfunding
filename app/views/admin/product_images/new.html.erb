<% @title = '商品画像の新規登録' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for [ :admin, @product, @product_image ] do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons">
      <%= f.submit '登録' %>
      <%= link_to 'キャンセル', :admin_product_product_images %>
    </div>
  <% end %>
</div>
